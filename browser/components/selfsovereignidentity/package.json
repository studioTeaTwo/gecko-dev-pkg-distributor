    "chokidar-cli": "^3.0.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
