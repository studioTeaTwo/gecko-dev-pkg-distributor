    "watch:sass": "chokidar \"content/panels/**/*.scss\" -c \"npm run build:sass\"",
    "lint": "eslint --ext .ts,.tsx,mjs ./{src,actors}",
    "lint:fix": "eslint --fix --ext .ts,.tsx,mjs ./{src,actors}",
